{
  "name": "fullstack",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "install:client": "cd client && npm install && typings install",
    "install:server": "cd server && npm install && typings install",
    "postinstall": "npm run install:client && npm run install:server",
    "build:client": "cd client && gulp build",
    "clean:client": "cd client && gulp clean",
    "watch:client": "cd client && gulp watch",
    "build:server": "gulp build:server",
    "clean:server": "gulp clean:server",
    "watch:server": "gulp watch:server"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-add-src": "^0.2.0",
    "gulp-rev": "^7.1.2",
    "gulp-typescript": "^3.1.3",
    "typings": "^2.0.0"
  }
}
